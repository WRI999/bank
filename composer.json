{
    "name": "assetkita/bank",
    "description": "Asset Kita bank services package.",
    "keywords": [
      "assetkita",
      "laravel",
      "bank",
      "fintech"
    ],
    "homepage": "https://assetkita.com",
    "license": "MIT",
    "authors": [
      {
        "name": "Agung Trilaksono",
        "email": "agungtrilaksono899@hotmail.com",
        "role": "Software Engineer"
      }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "6.3.3",
        "illuminate/contracts": "5.5.*",
        "illuminate/http": "5.5.*",
        "illuminate/support": "5.5.*",
        "illuminate/validation": "5.5.*",
        "psr/http-message": "1.0.*"
    },
    "require-dev": {
      "fzaninotto/faker": "~1.4",
      "orchestra/testbench": "3.5",
      "phpunit/phpunit": "~6.0"
    },
    "autoload": {
      "psr-4": {
        "Assetku\\BankService\\": "src/"
      },
      "files": [
        "src/helpers.php"
      ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "Assetku\\BankService\\Providers\\BankServiceProvider"
        ],
        "aliases": {
          "Bank": "Assetku\\BankService\\Facades\\Bank"
        }
      }
    },
    "config": {
      "sort-packages": true
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
