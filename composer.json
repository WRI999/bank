{
  "name": "assetkita/bank",
  "description": "Asset Kita bank services package.",
  "keywords": [
    "laravel",
    "bank",
    "permata bank"
  ],
  "homepage": "https://assetkita.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Agung Trilaksono",
      "email": "agungtrilaksono899@hotmail.com",
      "role": "Software Engineer"
    },
    {
      "name": "Yoel Peter Chandra",
      "email": "yoelpc4@gmail.com",
      "role": "Full Stack Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.5.2",
    "illuminate/contracts": "5.5.*",
    "illuminate/support": "5.5.*",
    "illuminate/validation": "5.5.*",
    "psr/http-message": "^1.0.1",
    "symfony/http-foundation": "^3.4.37",
    "symfony/http-kernel": "v3.4.37"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.9.1",
    "orchestra/testbench": "^3.5.5",
    "phpunit/phpunit": "^6.5.14"
  },
  "autoload": {
    "psr-4": {
      "Assetku\\BankService\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Assetku\\BankService\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Assetku\\BankService\\Providers\\BankServiceProvider"
      ],
      "aliases": {
        "Bank": "Assetku\\BankService\\Facades\\Bank"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
